{
  "version": "1.0.0",
  "source": "./index.html",
  "type": "module",
  "name": "anima-project",
  "description": "A React project automatically generated by Anima using the Shadcn UI library",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "source": "dist",
    "publicPath": "/impulse-new",           
    "port": 45678,                          
    "crawl": false,                         
    "include": [
      "/",
      "/about-us",
      "/services",
      "/services/content-writing-services",
      "/services/performance-marketing",
      "/services/social-media-marketing",
      "/services/search-engine-optimization",
      "/services/search-engine-optimization/enterprise-seo-services",
      "/services/search-engine-optimization/ecommerce-seo-services",
      "/services/search-engine-optimization/b2b-seo-services",
      "/services/search-engine-optimization/local-seo-services",
      "/services/website-development",
      "/services/branding-creative-services",
      "/services/employee-branding-agency",
      "/services/corporate-communication-agency",
      "/services/social-media-video-production",
      "/ai-seo-agency",
      "/career",
      "/contact-us",
      "/digital-marketing-agency-in-india",
      "/digital-marketing-agency-in-thane",
      "/digital-marketing-agency-in-navi-mumbai",
      "/digital-marketing-agency-in-pune"
    ],
    "skipThirdPartyRequests": true,
    "puppeteerArgs": ["--no-sandbox"]
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.453.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-numbers": "^3.0.9",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.9.1",
    "swiper": "^12.0.2",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@vitejs/plugin-react": "4.3.4",
    "esbuild": "0.24.0",
    "gh-pages": "^6.3.0",
